                        generic-name="kiwihscale"
                        title="Kiwi HScale"/>
    <glade-widget-class name="ProxyVScale"
                        generic-name="kiwivscale"
                        title="Kiwi VScale"/>
    <glade-widget-class name="ProxyColorButton"
                        generic-name="kiwicolorbutton"
                        title="Kiwi ColorButton"/>

    <!-- Deprecated, but kept for compatibility -->
    <glade-widget-class name="ProxyComboBoxEntry"
                        generic-name="kiwicomboboxentry"
                        title="Kiwi Combo Box Entry"/>

